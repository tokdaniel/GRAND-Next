{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "start": "next start",
    "prettier": "pretty-quick --pattern \"./**/*.{ts,tsx,md}\"",
    "generate": "graphql-codegen",
    "check-types": "tsc --noEmit",
    "check-format": "npm run prettier --check --verbose",
    "check-lint": "eslint --ignore-path .gitignore \"./**/*.{ts,tsx}\"",
    "format": "npm run prettier --staged",
    "lint": "eslint --ignore-path ../.gitignore --fix \"./**/*.{ts,tsx}\"",
    "validate": "npm-run-all --parallel check-types check-format check-lint"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@apollo/react-hooks": "^4.0.0",
    "@neo4j/graphql": "^1.0.0-alpha.3",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "apollo-server-micro": "^2.19.2",
    "autoprefixer": "^9.8.6",
    "cross-fetch": "^3.0.6",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^7.0.2",
    "neo4j-driver": "^4.2.2",
    "next": "latest",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "postcss": "^7.0.35",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@next/bundle-analyzer": "^10.0.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "fast-check": "^2.12.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "twin.macro": "^2.1.1",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
